{
  "cli": {
    "version": ">= 2.6.0",
    "appVersionSource": "local"
  },
  "build": {
    "test": {
      "channel": "test",
      "android": {
        "gradleCommand": ":app:assembleRelease :app:assembleAndroidTest -DtestBuildType=release",
        "withoutCredentials": true
      },
      "ios": {
        "simulator": true
      }
    },
    "development": {
      "developmentClient": true,
      "distribution": "internal",
      "corepack": "true",
      "ios": {
        "simulator": true
      }
    },
    "preview": {
      "distribution": "internal",
      "channel": "preview",
      "corepack": "true",
      "ios": {
        "simulator": true
      },
      "android": {
        "buildType": "apk",
        "corepack": "true",
        "gradleCommand": ":app:assembleRelease",
        "distribution": "internal"
      },
      "env": {
        "ANDROID_SDK_ROOT": "/Users/emil/Library/Android/sdk"
      }
    },
    "production": {
      "channel": "production",
      "developmentClient": false,
      "distribution": "store",
      "corepack": true,
      "ios": {
        "simulator": false
      }
    },
    "android": {
      "corepack": true,
      "distribution": "store"
    }
  },
  "submit": {
    "production": {
      "ios": {
        "appleId": "emilfrom@gmail.com",
        "ascAppId": "6755256395",
        "appleTeamId": "K2L2XPK739"
      },
      "android": {
        "serviceAccountKeyPath": "./playstore_secret.json",
        "track": "internal"
      }
    }
  }
}